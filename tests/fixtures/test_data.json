  "integration_test_scenarios": [
    {
      "name": "CRM Integration Success",
      "description": "Test successful CRM contact creation",
      "input": {
        "email": "integration.test@example.com",
        "first_name": "Integration",
        "last_name": "Test",
        "company": "Test Company"
      },
      "mock_responses": {
        "hubspot_create_contact": {
          "status": 200,
          "body": {
            "id": "test_contact_123",
            "properties": {
              "email": "integration.test@example.com",
              "firstname": "Integration",
              "lastname": "Test",
              "company": "Test Company"
            },
            "createdAt": "2024-01-15T10:30:00Z"
          }
        }
      },
      "expected_outcome": {
        "status": "success",
        "contact_id": "test_contact_123",
        "created": true
      }
    },
    {
      "name": "Email Notification Success",
      "description": "Test successful email notification sending",
      "input": {
        "to": "test@example.com",
        "subject": "Test Subject",
        "body": "Test email body"
      },
      "mock_responses": {
        "gmail_send": {
          "status": 200,
          "body": {
            "id": "test_message_456",
            "threadId": "test_thread_789",
            "labelIds": ["SENT"]
          }
        }
      },
      "expected_outcome": {
        "status": "success",
        "message_id": "test_message_456",
        "sent": true
      }
    },
    {
      "name": "Slack Notification Success",
      "description": "Test successful Slack message posting",
      "input": {
        "channel": "#test-channel",
        "text": "Test message",
        "attachments": [
          {
            "color": "good",
            "title": "Test Attachment",
            "text": "Test attachment content"
          }
        ]
      },
      "mock_responses": {
        "slack_post_message": {
          "status": 200,
          "body": {
            "ok": true,
            "channel": "C1234567890",
            "ts": "1234567890.123456",
            "message": {
              "text": "Test message",
              "user": "U1234567890",
              "ts": "1234567890.123456"
            }
          }
        }
      },
      "expected_outcome": {
        "status": "success",
        "message_ts": "1234567890.123456",
        "posted": true
      }
    }
  ],
  
  "error_scenarios": [
    {
      "name": "API Rate Limit Error",
      "description": "Test handling of API rate limit errors",
      "trigger": "rate_limit_exceeded",
      "mock_response": {
        "status": 429,
        "body": {
          "error": "rate_limited",
          "message": "Too many requests. Try again in 60 seconds."
        }
      },
      "expected_behavior": {
        "retry_attempted": true,
        "retry_delay": "exponential_backoff",
        "max_retries": 3,
        "final_action": "dead_letter_queue"
      }
    },
    {
      "name": "Authentication Error",
      "description": "Test handling of authentication failures",
      "trigger": "invalid_credentials",
      "mock_response": {
        "status": 401,
        "body": {
          "error": "unauthorized",
          "message": "Invalid API key"
        }
      },
      "expected_behavior": {
        "retry_attempted": false,
        "error_logged": true,
        "alert_sent": true,
        "fallback_action": "manual_review_queue"
      }
    },
    {
      "name": "Service Unavailable Error",
      "description": "Test handling of service unavailability",
      "trigger": "service_down",
      "mock_response": {
        "status": 503,
        "body": {
          "error": "service_unavailable", 
          "message": "Service temporarily unavailable"
        }
      },
      "expected_behavior": {
        "retry_attempted": true,
        "retry_delay": "exponential_backoff",
        "max_retries": 3,
        "circuit_breaker_triggered": true
      }
    }
  ]
}