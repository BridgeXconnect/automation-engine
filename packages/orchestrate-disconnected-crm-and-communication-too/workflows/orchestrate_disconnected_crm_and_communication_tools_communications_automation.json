{
  "name": "orchestrate_disconnected_crm_and_communication_tools_communications_automation",
  "nodes": [
    {
      "id": "new-customer-trigger",
      "name": "webhook_new_customer_signup",
      "type": "n8n-nodes-base.webhook",
      "position": [
        200.0,
        300.0
      ],
      "parameters": {
        "path": "new-customer",
        "httpMethod": "POST",
        "responseMode": "onReceived"
      }
    },
    {
      "id": "welcome-email",
      "name": "send_welcome_email",
      "type": "n8n-nodes-base.emailSend",
      "position": [
        400.0,
        300.0
      ],
      "parameters": {
        "to": "={{$json.email}}",
        "subject": "Welcome to {{$json.company_name}}!",
        "html": "<h1>Welcome!</h1><p>Thank you for signing up. We're excited to work with you.</p>"
      }
    },
    {
      "id": "create-project",
      "name": "setup_project_workspace",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        600.0,
        300.0
      ],
      "parameters": {
        "url": "{{$env.PROJECT_API_URL}}/projects",
        "method": "POST",
        "headers": {
          "Authorization": "Bearer {{$credentials.project_api.token}}"
        },
        "body": {
          "customer_id": "={{$json.customer_id}}",
          "project_name": "={{$json.company_name}} - Onboarding",
          "template": "standard_onboarding"
        }
      }
    },
    {
      "id": "schedule-kickoff",
      "name": "schedule_kickoff_call",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        800.0,
        300.0
      ],
      "parameters": {
        "url": "{{$env.CALENDAR_API_URL}}/events",
        "method": "POST",
        "headers": {
          "Authorization": "Bearer {{$credentials.calendar_api.token}}"
        },
        "body": {
          "title": "Kickoff Call - {{$json.company_name}}",
          "duration": 60,
          "attendees": [
            "={{$json.email}}",
            "sales@company.com"
          ]
        }
      }
    },
    {
      "id": "lead_qualification_workflow_webhook-trigger",
      "name": "webhook_lead_qualification_workflow_webhook_trigger",
      "type": "n8n-nodes-base.webhook",
      "position": [
        600.0,
        300.0
      ],
      "parameters": {
        "path": "lead-intake",
        "httpMethod": "POST",
        "responseMode": "onReceived"
      }
    },
    {
      "id": "lead_qualification_workflow_validate-data",
      "name": "lead_qualification_workflow_validate_lead_data",
      "type": "n8n-nodes-base.set",
      "position": [
        800.0,
        300.0
      ],
      "parameters": {
        "values": {
          "string": [
            {
              "name": "status",
              "value": "validated"
            }
          ]
        }
      }
    },
    {
      "id": "lead_qualification_workflow_crm-create",
      "name": "lead_qualification_workflow_create_crm_record",
      "type": "n8n-nodes-base.httpRequest",
      "position": [
        1000.0,
        300.0
      ],
      "parameters": {
        "url": "https://api.crm-system.com/leads",
        "method": "POST",
        "headers": {
          "Authorization": "Bearer {{$credentials.crm_api.token}}"
        }
      }
    },
    {
      "id": "lead_qualification_workflow_send-notification",
      "name": "lead_qualification_workflow_send_notification",
      "type": "n8n-nodes-base.emailSend",
      "position": [
        1200.0,
        300.0
      ],
      "parameters": {
        "to": "sales@company.com",
        "subject": "New Lead: {{$json['company_name']}}",
        "message": "New lead received and processed automatically."
      }
    },
    {
      "id": "lead_qualification_workflow_error-handler",
